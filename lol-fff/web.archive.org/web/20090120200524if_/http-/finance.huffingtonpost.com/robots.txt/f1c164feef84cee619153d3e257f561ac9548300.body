<style>
 #fc_popup {
  z-index:10000;
  position: absolute; 
  display: none; 
  top: 35px; 
  left: 35px; 
  background: white; 
  border: 1px solid #AAA;
  padding: 0px;
  margin: 0px;
  overflow:hidden;
 }
 #fc_popup_title {
  z-index:91000;
  height: 24px; 
  vertical-align: middle; 
  line-height: 24px; 
  text-align: right; 
  background-color: #CCC; 
  border-bottom: 1px solid #AAA;
 }
 #fc_popup_iframe {
  z-index:92000;
  border: 0px;
  padding: 0px;
  margin: 0px;
  background-color: #FFFFFF;
 }
</style>

<div id="fc_popup" class="fc">
 <div id="fc_popup_title"><a href="#" onclick="closeFCPopup(); return false">CLOSE WINDOW</a>&nbsp;&nbsp;</div>
 <iframe id="fc_popup_iframe"></iframe>
</div>

<script>

function openFCPopup (url) {
 document.getElementById('fc_popup_iframe').src = url;
 document.getElementById('fc_popup').style.display = 'block';
 //alert(fc_popup.innerHTML);
 return false;
}

function closeFCPopup () {
 document.getElementById('fc_popup_iframe').src = '';
 document.getElementById('fc_popup').style.display = 'none';
 return false;
}

 function findPopables() {
  var nodes = document.getElementsByTagName('A');
  var i;
  for (i in nodes) {
   var node = nodes[i];
   if (node.getAttribute && node.getAttribute('popable') == 'true') {
    eval("node.onclick = function () { openFCPopup('" + node.href + "'); return false; }");
   }
  }
 }

setTimeout('findPopables()',10);



var popupTimer = null;
function runPopupTimer() {
 if (popupTimer != null) clearTimeout(popupTimer);
 calcPopupPosition();
 
 popupTimer = setTimeout('runPopupTimer()',1000);
}
runPopupTimer();

function calcPopupPosition() {
 var fc_popup = document.getElementById('fc_popup');
 var fc_popup_iframe = document.getElementById('fc_popup_iframe');

 var windowSize = getWindowDimensions();
 var scrollOffset = getScrollOffset();
 
 fc_popup.style.top = (scrollOffset.y + 35) + "px";
 fc_popup.style.left = (scrollOffset.x + 35) +"px";
 
 fc_popup.style.width = (windowSize.x - 35 - 35) + "px";
 fc_popup_iframe.style.width = (windowSize.x - 35 - 35) + "px";
 fc_popup_iframe.style.height = (windowSize.y - 35 - 61) + "px";
}

function getWindowDimensions() {
var x,y;
if (self.innerHeight) // all except Explorer
{
	x = self.innerWidth;
	y = self.innerHeight;
}
else if (document.documentElement && document.documentElement.clientHeight)
	// Explorer 6 Strict Mode
{
	x = document.documentElement.clientWidth;
	y = document.documentElement.clientHeight;
}
else if (document.body) // other Explorers
{
	x = document.body.clientWidth;
	y = document.body.clientHeight;
}

return {x: x, y: y};
}

function getScrollOffset() {
var x,y;
if (self.pageYOffset) // all except Explorer
{
	x = self.pageXOffset;
	y = self.pageYOffset;
}
else if (document.documentElement && document.documentElement.scrollTop)
	// Explorer 6 Strict
{
	x = document.documentElement.scrollLeft;
	y = document.documentElement.scrollTop;
}
else if (document.body) // all other Explorers
{
	x = document.body.scrollLeft;
	y = document.body.scrollTop;
}

return {x: x, y: y};
}


window.onscroll = calcPopupPosition;
window.onresize = calcPopupPosition;
</script>

