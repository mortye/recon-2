<script>
function findPos(obj) {
        var curleft = curtop = 0;
        if (obj.offsetParent) {
                curleft = obj.offsetLeft
                curtop = obj.offsetTop
                while (obj = obj.offsetParent) {
                        curleft += obj.offsetLeft
                        curtop += obj.offsetTop
                }
        }
        return {y:curtop,x:curleft};
}


function format(number,decimals) {
  var i,d;

  // set default values
  if(number=="") number=parseInt("0");
  if(decimals=="") decimals=parseInt("2");

  // round number to specified number of decimals
  number=""+Math.round(number*Math.pow(10,decimals))*Math.pow(10,-decimals);

  // find index of decimal point
  d=number.indexOf(".");

  // if no decimal point, number is an integer
  // pad number with trailing zeros
  if(d==-1) {
    number=number+".";
    for(i=0;i<decimals;i++)
      number=number+"0";
    return number;
  }

  // if decimal point is first index
  // pad number with leading zero
  if(d==0) {
    number="0"+number;
    d++;
  }

  // if first index is negative sign
  // pad number with leading zero
  if(d==1 && number.substring(0,1)=="-") {
    number="-0" + number.substring(1,number.length);
    d++;
  }

  // truncate number to desired length
  number=number.substring(0,d+decimals+1);

  // if number is shorter than desired length
  // pad number with trailing zeros
  while(number.length<=d+decimals)
    number=number+"0";
  return number;
}

function addSymbolToWatchlist (ticker) {
 return _addSymbolToCookie('fc_watchlist',ticker);
}
function delSymbolFromWatchlist (ticker) {
 return _delSymbolFromCookie('fc_watchlist',ticker);
}
function getSymbolsFromWatchlist () {
 return _getSymbolsFromCookie('fc_watchlist');
}

function addSymbolToRecentStocks (ticker) {
 return _addSymbolToCookie('fc_recent_stocks',ticker);
}
function delSymbolFromRecentStocks (ticker) {
 return _delSymbolFromCookie('fc_recent_stocks',ticker);
}
function getSymbolsFromRecentStocks () {
 return _getSymbolsFromCookie('fc_recent_stocks');
}

function _addSymbolToCookie (cookieName,ticker) {
 var stocks = getCookie(cookieName).split(',');
 stocks.push(ticker);

 var stockHash = new Array();
 for (var i = 0; i < stocks.length; i++) {
  stockHash[stocks[i]] = 1;
 }
 
 var newstocks = '';
 for (var ticker in stockHash) {
  if (ticker.length > 0) {
   newstocks += ticker + ',';
  }
 }
 setCookie(cookieName,newstocks.toUpperCase());
}

function _delSymbolFromCookie (cookieName,ticker) {
 var stocks = getCookie(cookieName).toUpperCase().split(',');
 var newstocks = '';
 for (var i = 0; i < stocks.length; i++) {
  if (ticker.toUpperCase() != stocks[i]) {
   newstocks += stocks[i] + ',';
  }
 }
 setCookie(cookieName,newstocks.toUpperCase());
}

function _getSymbolsFromCookie (cookieName) {
 return getCookie(cookieName).toUpperCase().split(',');
}

function getCookie(name) {
  var dc = document.cookie;
  var prefix = name + "=";
  var begin = dc.indexOf("; " + prefix);
  if (begin == -1) {
    begin = dc.indexOf(prefix);
    if (begin != 0) return "";
  } else
    begin += 2;
  var end = document.cookie.indexOf(";", begin);
  if (end == -1)
    end = dc.length;
  return unescape(dc.substring(begin + prefix.length, end));
}
function setCookie(name, value) {
  var curCookie = name + "=" + value + '; expires=Sun, 17-Jan-2038 19:14:07 GMT; path=/';
  document.cookie = curCookie;
}


</script>

<script>
 function hoverableOver(el) {
  el.oldClassName = el.className;
  el.className += ' hover';
 }

 function hoverableOut (el) {
  el.className = el.oldClassName;
 }

 function findHoverables(el) {
  var nodes = el.childNodes;
  var i;
  var hoverMenu;
  for (i in nodes) {
   var node = nodes[i];
   if(node.childNodes && node.childNodes.length > 0) {
    findHoverables(node);
   }
   if (node.getAttribute && node.getAttribute('hoverable') == 'true') {
    node.setAttribute('hoverable','processed');
    addHoverListeners(node);
   }
   if (node.getAttribute && node.getAttribute('hoverMenu') != null) {
    var hoverMenuName = node.getAttribute('hoverMenu');
    var hoverMenu;
    if (hoverMenuName == '_next') {
     var n = node;
     while ((n = n.nextSibling) && (n.nodeType != 1)) {};
     hoverMenu = n;
    } else {
     hoverMenu = document.getElementById(hoverMenuName);
    }
    node.removeAttribute('hoverMenu');
    if (hoverMenu) {
     addHoverMenuListeners(node,hoverMenu);
    }
   }

  }
 }

 function addHoverListeners (el) {
  if (el.addEventListener) {
   el.addEventListener ("mouseover",function() {hoverableOver(el)},false);
   el.addEventListener ("mouseout",function() {hoverableOut(el)},false);
  } else if (el.attachEvent) {
   el.attachEvent ("onmouseover",function() {hoverableOver(el)});
   el.attachEvent ("onmouseout",function() {hoverableOut(el)});
  } else {
   el.onmouseover = function() {hoverableOver(el)};
   el.onmouseout = function() {hoverableOut(el)}; 
  }
 }

 var currentHoverMenu;
 
 function addHoverMenuListeners (el, menu) {
 with(menu) {
  var overFunc = function () {
   if (currentHoverMenu != null && currentHoverMenu != menu) {
    currentHoverMenu.style.display = 'none';
   }
   currentHoverMenu = menu;
   menu.hoverClose = 0;
   menu.style.display = '';
  };
  var outRealFunc = function () {
   if (menu.hoverClose == 1) { 
    menu.style.display = 'none';
   }
  };
  var outFunc = function () {
   menu.hoverClose = 1;
   setTimeout( outRealFunc, 300);
  };
  _hoverableAddListeners(el,overFunc,outFunc);
  _hoverableAddListeners(menu,overFunc,outFunc);
  }
 }

 function _hoverableAddListeners (el,overFunc, outFunc) {
  if (el.addEventListener) {
   el.addEventListener ("mouseover",overFunc,false);
   el.addEventListener ("mouseout",outFunc,false);
  } else if (el.attachEvent) {
   el.attachEvent ("onmouseover",overFunc);
   el.attachEvent ("onmouseout",outFunc);
  } else {
   el.onmouseover = overFunc;
   el.onmouseout = outFunc; 
  }
 }


 setTimeout("findHoverables(document.body)",1);
 setTimeout("findHoverables(document.body)",2000);
 setTimeout("findHoverables(document.body)",5000);
 setTimeout("findHoverables(document.body)",25000);
</script>


