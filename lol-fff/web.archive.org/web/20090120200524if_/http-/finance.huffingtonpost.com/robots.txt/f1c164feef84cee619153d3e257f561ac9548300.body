function tickerPopupSetQuote(quote) {
  document.getElementById('tp_header').innerHTML = quote['Name'];

  if (quote['Last'] > 0 && quote['PrevClose'] > 0) {
   var changePrice = quote['Last'] - quote['PrevClose'];
   var changePercent = changePrice * 100 / quote['Last'];
  
   changePrice = format(changePrice,2);
   changePercent = format(changePercent,2);

   var tp_quotes = document.getElementById('tp_quote').getElementsByTagName('span');
   tp_quotes[0].innerHTML = quote['Last'];
   tp_quotes[1].innerHTML = changePrice;
   tp_quotes[2].innerHTML = changePercent;
   tp_quotes[3].innerHTML = quote['Open'];
   tp_quotes[4].innerHTML = quote['Low'];
   tp_quotes[5].innerHTML = quote['High'];
  }
}

